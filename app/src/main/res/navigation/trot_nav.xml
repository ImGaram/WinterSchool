<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/trot_nav"
    app:startDestination="@id/trot1Fragment">

    <fragment
        android:id="@+id/trot1Fragment"
        android:name="com.example.winterschool.singer.Trot1Fragment"
        android:label="fragment_trot1"
        tools:layout="@layout/fragment_trot1" >
        <action
            android:id="@+id/action_trot1Fragment_to_trot2Fragment"
            app:destination="@id/trot2Fragment" />
        <action
            android:id="@+id/action_trot1Fragment_to_trot3Fragment"
            app:destination="@id/trot3Fragment" />
    </fragment>
    <fragment
        android:id="@+id/trot2Fragment"
        android:name="com.example.winterschool.singer.Trot2Fragment"
        android:label="fragment_trot2"
        tools:layout="@layout/fragment_trot2" >
        <action
            android:id="@+id/action_trot2Fragment_to_trot1Fragment"
            app:destination="@id/trot1Fragment" />
        <action
            android:id="@+id/action_trot2Fragment_to_trot3Fragment"
            app:destination="@id/trot3Fragment" />
    </fragment>
    <fragment
        android:id="@+id/trot3Fragment"
        android:name="com.example.winterschool.singer.Trot3Fragment"
        android:label="fragment_trot3"
        tools:layout="@layout/fragment_trot3" >
        <action
            android:id="@+id/action_trot3Fragment_to_trot1Fragment"
            app:destination="@id/trot1Fragment" />
        <action
            android:id="@+id/action_trot3Fragment_to_trot1Fragment2"
            app:destination="@id/trot1Fragment" />
        <action
            android:id="@+id/action_trot3Fragment_to_trot2Fragment"
            app:destination="@id/trot2Fragment" />
    </fragment>
</navigation>